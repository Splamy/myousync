    "postinstall": "bun2nix -o bun.nix"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.2.15",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-load-config": "^6.0.1",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@rsbuild/plugin-svelte": "^1.0.8",
    "svelte-ux": "^1.0.9"
  },
  "trustedDependencies": [
    "core-js",
    "svelte-preprocess"
  ]
